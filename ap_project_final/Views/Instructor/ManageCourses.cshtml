@model IEnumerable<ap_project_final.Models.Course>

@{
    ViewBag.Title = "Manage Courses";
}

<h2>Manage Courses</h2>

<ul>
    @foreach (var course in Model)
    {
        <li>
            <a href="/Instructor/ManageParticipants/@course.Id">@course.CourseCode</a>
        </li>
    }
</ul>

<a href="/Instructor/Index">Back</a>